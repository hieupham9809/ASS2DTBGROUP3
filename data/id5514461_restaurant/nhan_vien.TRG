triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `trigger_add_emp` AFTER INSERT ON `nhan_vien` FOR EACH ROW BEGIN\n	DECLARE t_role VARCHAR(45);\n    SET t_role = (SELECT `role`\n                  FROM `user`\n                  WHERE `user`.`id`= NEW.`MA_SO_NHAN_VIEN`);\n    IF t_role = \'shipper\' THEN\n    	INSERT INTO `NHAN_VIEN_SHIPPER`(`MA_SO_NHAN_VIEN`)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'chief\' THEN\n    	INSERT INTO NHAN_VIEN_BEP(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'orderman\' THEN\n    	INSERT INTO NHAN_VIEN_BOI_BAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'accountant\' THEN\n    	INSERT INTO NHAN_VIEN_KE_TOAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'manager\' THEN\n    	INSERT INTO NHAN_VIEN_QUAN_LI(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'waiter\' THEN\n    	INSERT INTO NHAN_VIEN_BOI_BAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'cashier\' THEN\n    	INSERT INTO `NHAN_VIEN_THU_NGAN`(`MA_SO_NHAN_VIEN`)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `trigger_add_emp` AFTER INSERT ON `nhan_vien` FOR EACH ROW BEGIN\n	DECLARE t_role VARCHAR(45);\n    SET t_role = (SELECT `role`\n                  FROM `user`\n                  WHERE `user`.`id`= NEW.`MA_SO_NHAN_VIEN`);\n    IF t_role = \'shipper\' THEN\n    	INSERT INTO `NHAN_VIEN_SHIPPER`(`MA_SO_NHAN_VIEN`)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'chief\' THEN\n    	INSERT INTO NHAN_VIEN_BEP(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'orderman\' THEN\n    	INSERT INTO NHAN_VIEN_BOI_BAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'accountant\' THEN\n    	INSERT INTO NHAN_VIEN_KE_TOAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'manager\' THEN\n    	INSERT INTO NHAN_VIEN_QUAN_LI(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    ELSEIF t_role = \'waiter\' THEN\n    	INSERT INTO NHAN_VIEN_BOI_BAN(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n     ELSEIF t_role = \'order\' THEN\n    	INSERT INTO NHAN_VIEN_ORDER(MA_SO_NHAN_VIEN)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);    \n    ELSEIF t_role = \'cashier\' THEN\n    	INSERT INTO `NHAN_VIEN_THU_NGAN`(`MA_SO_NHAN_VIEN`)\n        VALUE(NEW.`MA_SO_NHAN_VIEN`);\n    END IF;\nEND'
